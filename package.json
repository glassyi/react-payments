    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "craco-alias": "^3.0.1",
    "eslint": "^8.6.0",
    "eslint-plugin-storybook": "^0.5.5",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0"
